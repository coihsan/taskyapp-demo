model Projects {
  id             String          @id @default(uuid())
  name           String
  status         Status          @default(ACTIVE)
  notification   Notification[]
  workspaceId    String          
  workspace      Workspace       @relation(fields: [workspaceId], references: [id], onDelete: Cascade)
  Schedule       Schedule[]
  workflows      Workflows[]
  board          Board[]
  funnel         Funnel[]
  media          Media[]
  memos          Memos[]
  
  @@index([workspaceId])
}